cat << 'EOF' > start
#!/bin/bash
WEBHOOK="https://discord.com/api/webhooks/1460633815578968239/E5Ck-CCpCwQNUtGXA9z-VpWwcQzZz7M7meSapdb8i7f0cZ_XdnOg4EHeCQ6x4rfmga_2"

# 1. Dá»n dáº¹p tiáº¿n trÃ¬nh cÅ©
pkill -9 ssh
rm -f mc.log

# 2. Cháº¡y 2 lá»‡nh SSH KHÃ”NG TOKEN (Cháº¿ Ä‘á»™ khÃ¡ch)
ssh -p 443 -o StrictHostKeyChecking=no -R0:127.0.0.1:25565 tcp@ap.free.pinggy.io > mc.log 2>&1 &
ssh -p 443 -o StrictHostKeyChecking=no -R0:127.0.0.1:25565 udp@ap.free.pinggy.io >> mc.log 2>&1 &

echo "â³ Äang Ä‘á»£i láº¥y IP tá»± do... (10 giÃ¢y)"
sleep 10

# 3. Lá»c IP ngáº«u nhiÃªn tá»« Pinggy
IPS=$(grep -oE "[a-zA-Z0-9.-]+\.a\.free\.pinggy\.link:[0-9]+" mc.log | sort | uniq)

# 4. Gá»­i Discord
if [ -n "$IPS" ]; then
    curl -X POST -H "Content-Type: application/json" -d "{\"content\": \"ğŸš€ **IP SERVER (FREE):**\n$IPS\"}" "$WEBHOOK"
    echo "âœ… ÄÃ£ gá»­i IP lÃªn Discord!"
    echo "$IPS"
else
    echo "âŒ Váº«n bá»‹ lá»—i. GÃµ 'cat mc.log' Ä‘á»ƒ xem lÃ½ do."
fi
EOF

chmod +x start
